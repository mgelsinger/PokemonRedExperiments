{
  "name": "exploration_basic",
  "description": "Explore Pallet Town and Route 1 - focus on discovering new tiles",
  "env": {
    "headless": true,
    "save_final_state": false,
    "early_stop": false,
    "action_freq": 24,
    "max_steps": 81920,
    "print_rewards": true,
    "save_video": false,
    "fast_video": true,
    "debug": false,
    "reward_scale": 1.0,
    "explore_weight": 1.0,
    "reward_config": {
      "exploration_new_tile": 5.0,
      "exploration_recent_tile": 0.5,
      "battle_hp_delta": 0.0,
      "battle_win": 0.0,
      "battle_loss": 0.0,
      "milestone_badge": 50.0,
      "milestone_level_up": 1.0,
      "milestone_key_location": 20.0,
      "milestone_event": 2.0,
      "penalty_step": -0.001,
      "penalty_wall": -0.05,
      "penalty_stuck": -0.1,
      "reward_scale": 1.0
    }
  },
  "train": {
    "num_envs": 16,
    "total_multiplier": 200,
    "batch_size": 512,
    "n_epochs": 3,
    "gamma": 0.99,
    "ent_coef": 0.02,
    "learning_rate": 0.0005,
    "clip_range": 0.2,
    "vf_coef": 0.5,
    "max_grad_norm": 0.5,
    "gae_lambda": 0.95
  },
  "recommended_steps": {
    "smoke_test": 1000000,
    "early_learning": 10000000,
    "deeper_training": 50000000
  }
}
